type alias Bundle =
    { fsPermission : FileSystem.Permission
    , compendium : Compendium
    }



-- ðŸ› 


mapCompendium : (Compendium -> Compendium) -> Bundle -> Bundle
mapCompendium fn bundle =
    { bundle | compendium = fn bundle.compendium }
